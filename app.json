{
  "expo": {
    "name": "Hestia",
    "slug": "Hestia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./Img/Icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Hestia.HestiaApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Foreground_Hestia.png",
        "backgroundImage": "./assets/Background_Hestia.png"
      },
      "package": "com.Hestia.HestiaApp",
      "icon": "./assets/Logo_Hestia.png",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./Img/Icon.png",
          "color": "white",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ]
  }
}
