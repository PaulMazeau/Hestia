{
  "expo": {
    "name": "Hestia",
    "slug": "Hestia",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/Logo_Hestia.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/FirstScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Hestia.HestiaApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Foreground_Hestia.png",
        "backgroundImage": "./assets/Background_Hestia.png"
      },
      "package": "com.Hestia.HestiaApp",
      "icon": "./assets/Logo_Hestia.png",
      "softwareKeyboardLayoutMode": "pan",
      "versionCode": 2,
      "blockedPermissions": ["android.permission.ACCESS_COARSE_LOCATION", "android.permission.ACCESS_FINE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./Assets/IconNotification.png",
          "color": "#FFFFF",
          "mode": "production"
        }
      ]
    ]
  }
}
